  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "webpack build --mode production --node-env production",
    "dev": "webpack serve --mode development --node-env development",
    "build:gh-pages": "npm run build # legacy command, gh pages is not an independent process",
    "lint": "eslint src test",
    "hint": "hint http://localhost:4567",
    "posttest": "npm run lint",
