{
  "extends": ["braintree/client", "braintree/es6"],
  "parserOptions": {
    "sourceType": "script",
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "object-curly-spacing": [
      "error",
      "always",
      {
        "arraysInObjects": false,
        "objectsInObjects": false
      }
    ]
  },
  "ignorePatterns": ["babel.config.js", "wdio.conf.js", "webpack.config.js"]
}
